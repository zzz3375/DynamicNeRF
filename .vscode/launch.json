{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python 调试程序: 当前文件",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: generate_data.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/utils/generate_data.py",
            "args": [
                "--videopath",
                "DJI_20250516151729_0005_V.MP4"
            ],
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: NeRF Training",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run_nerf.py",
            "args": [
                "--config",
                "configs/config-WTB-plain-inservice.txt"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python: Render Results",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/render_results.py",
            "args": [
                "--output_dir", "render_results/WTB-Beijing",
                "--render_mode", "depth",
                "--make_video",
                "--config", "configs/config-WTB-Beijing-no_combine.txt",
                "--chunk", "16384"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "Python: scene_flow",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/analysis_scene_flow.py",
            "args": [
                // "--output_dir", "render_results/WTB-Beijing",
                // "--render_mode", "depth",
                // "--make_video",
                "--config", "configs/config-WTB-Beijing-no_combine.txt",
                // "--chunk", "16384"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        }
    ]
}