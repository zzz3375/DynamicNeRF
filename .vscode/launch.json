{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python 调试程序: 当前文件",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: generate_data.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/utils/generate_data.py",
            "args": [
                "--videopath",
                "DJI_20250516151729_0005_V.MP4",
                "--outputname",
                "DJI_20250516151729_0005_V"
            ],
            "cwd": "${workspaceFolder}/utils",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: NeRF Training",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run_nerf.py",
            "args": [
                "--config",
                "configs/config-WTB-inservice.txt"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python: Render Results",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/render_results.py",
            "args": [
                "--output_dir", "render_results",
                "--config", "configs/config-WTB-Beijing-no_combine.txt",
                "--chunk", "16384"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "Python: scene_flow",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/analysis_scene_flow.py",
            "args": [
                // "--output_dir", "render_results/WTB-Beijing",
                // "--render_mode", "depth",
                // "--make_video",
                "--config", "configs/config-WTB-Beijing-no_combine.txt",
                // "--chunk", "16384"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "Python: Render PLY",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/render_ply.py",
            "args": [
                "--output_dir", "render_ply",
                "--config", "configs/config-WTB-Beijing-no_combine.txt",
                "--chunk", "16384"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },        
        {
            "name": "Python: Evaluate Metrics",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/evaluate_metrics.py",
            "args": [
                "--result_dir",
                "render_results/DJI_20250516151729_0005_V-no_combine/rgb_map_s",
                "--gt_dir",
                "data/DJI_20250516151729_0005_V-100f-fps30/images",
                // "/mnt/c/Users/13694/gaussian-splatting/output/wind-blade-vibration-capture3-fps30-100/train/ours_30000/gt",
                "--mask_dir",
                "data/DJI_20250516151729_0005_V-100f-fps30/motion_masks",
                "--output",
                "results.xlsx"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
    ]
}